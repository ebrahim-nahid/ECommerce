@{
    ViewData["Title"] = "Category List";
}
<a asp-action="Create" class="btn btn-primary">Create New Category</a>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Description</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Description</td>
        </tr>
    }
   
</table>